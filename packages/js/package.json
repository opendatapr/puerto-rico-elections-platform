{
  "name": "prelecciones",
  "version": "0.1.0",
  "description": "Access Puerto Rico electoral data",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "puerto-rico",
    "elections",
    "electoral",
    "data",
    "geojson"
  ],
  "author": "Open Data Puerto Rico",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/opendatapr/puerto-rico-elections-platform.git",
    "directory": "packages/js"
  },
  "bugs": {
    "url": "https://github.com/opendatapr/puerto-rico-elections-platform/issues"
  },
  "homepage": "https://github.com/opendatapr/puerto-rico-elections-platform#readme",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
